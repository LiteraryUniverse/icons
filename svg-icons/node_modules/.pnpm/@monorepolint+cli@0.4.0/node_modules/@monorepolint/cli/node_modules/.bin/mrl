#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules/@monorepolint/cli/bin/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules/@monorepolint/cli/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules/@monorepolint/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules/@monorepolint/cli/bin/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules/@monorepolint/cli/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules/@monorepolint/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@monorepolint+cli@0.4.0/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/mrl" "$@"
else
  exec node  "$basedir/../../bin/mrl" "$@"
fi
