#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules/@storybook/html/bin/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules/@storybook/html/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules/@storybook/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules/@storybook/html/bin/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules/@storybook/html/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules/@storybook/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/@storybook+html@6.5.15_@babel+core@7.20.5_@storybook+builder-webpack5@6.5.15_@storybook+manag_esaazo6nh2kjgcpgi62ezoeiru/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/build.js" "$@"
else
  exec node  "$basedir/../../bin/build.js" "$@"
fi
