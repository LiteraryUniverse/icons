#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/tty-table@4.2.1/node_modules/tty-table/adapters/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/tty-table@4.2.1/node_modules/tty-table/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/tty-table@4.2.1/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/tty-table@4.2.1/node_modules/tty-table/adapters/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/tty-table@4.2.1/node_modules/tty-table/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/tty-table@4.2.1/node_modules:/home/storyteller/Web/lu-icons/svg-icons/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../adapters/terminal-adapter.js" "$@"
else
  exec node  "$basedir/../../adapters/terminal-adapter.js" "$@"
fi
